# Example .env file for VOS TOOL

# Whisper model size: tiny, base, small, medium, large
WHISPER_MODEL=base

# Primary language for transcription
LANGUAGE=en

# Accent adaptation (standard or egyptian)
ACCENT=egyptian

# Enable code-switching detection (True/False)
SUPPORT_CODE_SWITCHING=True

# (Optional) ReadyMode default URL
READYMODE_URL=https://resva.readymode.com/

# (Optional) Path to logs, data, and recordings (override defaults)
# LOG_DIR=logs
# DATA_DIR=data
# RECORDINGS_DIR=recordings

